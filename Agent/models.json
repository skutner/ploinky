        "gemini-2.5-flash": { "provider": "google", "modes": ["fast"] },
        "google/gemini-2.5-flash": { "provider": "openrouter", "modes": ["fast"] },
        "anthropic/claude-sonnet-4": { "provider": "openrouter", "modes": ["deep"] },
